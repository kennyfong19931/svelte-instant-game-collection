{
  "name": "svelte-instant-game-collection",
  "description": "PS Plus Instant game collection build with matyunya/smelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --basepath svelte-instant-game-collection",
    "start": "node __sapper__/build",
    "serve": "serve __sapper__/export",
    "cy:run": "cypress run",
    "lint": "prettier --write --plugin-search-dir=. ./src/**/*",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "now": "yarn build && now",
    "now-build": "sapper build --export",
    "update-api": "node src/updateData.js"
  },
  "pre-commit": [
    "lint"
  ],
  "browserslist": "last 3 version",
  "dependencies": {
    "compression": "^1.7.1",
    "dayjs": "^1.8.29",
    "express": "^4.17.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "sirv": "^0.4.6",
    "smelte": "^1.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@now/node": "^0.9.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.4",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "eslint-plugin-svelte3": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.0",
    "postcss-url": "^8.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.6.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.16",
    "svelte": "^3.23.2",
    "svelte-preprocess": "^2.9.0",
    "tailwindcss": "^1.4.6",
    "tailwindcss-elevation": "^0.3.4"
  }
}
